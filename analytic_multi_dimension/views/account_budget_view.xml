<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    
        <record id="crossovered_budget_view_form" model="ir.ui.view" >
            <field name="name">crossovered.budget.view.form</field>
            <field name="model">crossovered.budget</field>
            <field name="inherit_id" ref="account_budget.crossovered_budget_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='creating_user_id']" position="after">
                    <field name="dimension_group_id" />
                    <field name="d1_active" invisible="1"/>
                    <field name="d2_active" invisible="1"/>
                    <field name="d3_active" invisible="1"/>
                    <field name="d4_active" invisible="1"/>
                    <field name="d5_active" invisible="1"/>
                    <field name="d6_active" invisible="1"/>
                    <field name="d7_active" invisible="1"/>
                    <field name="d8_active" invisible="1"/>
                    <field name="d9_active" invisible="1"/>
                    <field name="d10_active" invisible="1"/>
                    <field name="d1" attrs="{'invisible': [('d1_active', '=', False)]}"/>
                    <field name="d2" attrs="{'invisible': [('d2_active', '=', False)]}"/>
                    <field name="d3" attrs="{'invisible': [('d3_active', '=', False)]}"/>
                    <field name="d4" attrs="{'invisible': [('d4_active', '=', False)]}"/>
                    <field name="d5" attrs="{'invisible': [('d5_active', '=', False)]}"/>
                    <field name="d6" attrs="{'invisible': [('d6_active', '=', False)]}"/>
                    <field name="d7" attrs="{'invisible': [('d7_active', '=', False)]}"/>
                    <field name="d8" attrs="{'invisible': [('d8_active', '=', False)]}"/>
                    <field name="d9" attrs="{'invisible': [('d9_active', '=', False)]}"/>
                    <field name="d10" attrs="{'invisible': [('d10_active', '=', False)]}"/>
                </xpath>
                <xpath expr="/form/sheet/notebook/page/field/tree/field[@name='analytic_account_id']" position="attributes">
                    <attribute name="readonly">1</attribute>
                </xpath>
                <xpath expr="/form/sheet/notebook/page/field/form//field[@name='analytic_account_id']" position="attributes">
                    <attribute name="domain">[('d1', '=', parent.d1)]</attribute>
                    <attribute name="domain">[('d2', '=', parent.d2)]</attribute>
                    <attribute name="domain">[('d3', '=', parent.d3)]</attribute>
                    <attribute name="domain">[('d4', '=', parent.d4)]</attribute>
                </xpath>
            </field>
        </record>         

     </data>
</openerp>
